{
  "name": "plaisio/console-assets",
  "description": "PhpPlaisio: Console Assets",
  "keywords": [
    "PhpPlaisio",
    "Console",
    "Assets"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-dom": "*",
    "ext-libxml": "*",
    "plaisio/console": "^3.1.0",
    "setbased/exception": "^2.4.0",
    "setbased/php-stratum": "^6.3.1",
    "setbased/php-stratum-sqlite-pdo": "^2.1.0",
    "symfony/console": "^6.4.0|^7.3.0",
    "symfony/filesystem": "^6.4.0|^7.3.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Console\\Assets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Console\\Assets\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
