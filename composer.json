{
  "name": "plaisio/console-assets",
  "description": "PhpPlaisio: Console Assets",
  "keywords": [
    "PhpPlaisio",
    "Console",
    "Assets"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-dom": "*",
    "ext-libxml": "*",
    "plaisio/console": "dev-master",
    "setbased/exception": "^2.3.0",
    "setbased/php-stratum": "^6.3.1",
    "setbased/php-stratum-sqlite-pdo": "^1.7.0",
    "symfony/console": "^6.2.5",
    "symfony/filesystem": "^6.2.5"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.6.1"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Console\\Assets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Console\\Assets\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
